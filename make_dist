rsync -rltv --delete --info=FLIST0,STATS0,BACKUP1 lib/fow/main.py ../dist/debian/usr/lib/fow/
rsync -rltv --delete --info=FLIST0,STATS0,BACKUP1 lib/fow/fow.py ../dist/debian/usr/lib/fow/
rsync -rltv --delete --info=FLIST0,STATS0,BACKUP1 lib/fow/plump.py ../dist/debian/usr/lib/fow/
rsync -rltv --delete --info=FLIST0,STATS0,BACKUP1 man/ ../dist/debian/usr/share/man/man1/
dpkg-deb --build ../dist/debian/
mv ../dist/debian.deb ../dist/fow_1.1-1_all.deb
